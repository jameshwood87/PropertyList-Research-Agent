# =================================
# AI PROPERTY RESEARCH AGENT - ENTERPRISE CONFIG
# =================================

# Core Database (PostgreSQL + PostGIS)
DATABASE_URL=postgres://propertylist:secure_password@localhost:5432/propertylist_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=propertylist
POSTGRES_PASSWORD=secure_password
POSTGRES_DB=propertylist_db

# Cache Layer (Redis)
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Search Engine (Elasticsearch)
ELASTIC_URL=http://localhost:9200
ELASTIC_INDEX=properties

# API Keys
OPENAI_API_KEY=your_openai_key_here
TAVILY_API_KEY=your_tavily_key_here
GOOGLE_MAPS_API_KEY=your_google_maps_key_here

# PropertyList.es XML Feed
XML_FEED_URL=http://propertylist-staging-assets-west.s3.eu-west-1.amazonaws.com/47/export/189963705804aee9/admin.xml
XML_UPDATE_API_KEY=super_secure_token_here
ALLOWED_PROPERTY_PUSHER_IP=123.123.123.123

# Scheduling & Performance
XML_UPDATE_CRON=30 2 * * *
CACHE_TTL=1800
DEFAULT_SEARCH_RADIUS=5
MAX_COMPARABLE_RESULTS=20

# Server Configuration
PORT=3004
NODE_ENV=development

# Performance Settings
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=20
CACHE_MAX_AGE=1800
CACHE_MAX_SIZE=1000

# Legacy SQLite (for migration period)
DATABASE_PATH=server/database/properties.db 