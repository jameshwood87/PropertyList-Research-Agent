# AI Property Research Agent - Environment Configuration Example
# Copy this to .env.local and fill in your actual API keys

# Database Configuration (Docker will use internal networking)
DATABASE_URL=postgres://propertylist:secure_password@postgres:5432/propertylist_db
REDIS_URL=redis://redis:6379
ELASTIC_URL=http://elasticsearch:9200

# API Keys - Replace with your actual keys
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here

# PropertyList API Credentials
PROPERTYLIST_API_USERNAME=your_propertylist_username
PROPERTYLIST_API_PASSWORD=your_propertylist_password

# OpenCage Geocoding API
OPENCAGE_API_KEY=your_opencage_api_key_here

# Application Configuration
NODE_ENV=production
PORT=3004
NEXT_PUBLIC_API_URL=http://localhost:3004

# Security
JWT_SECRET=your_jwt_secret_here
SESSION_SECRET=your_session_secret_here

# Optional: Analytics & Monitoring
ANALYTICS_ID=your_analytics_id_here 